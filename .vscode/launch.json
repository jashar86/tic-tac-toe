{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug TicTacToe",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/cpp/build/tictactoe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/cpp/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build TicTacToe",
			"miDebuggerPath": "/usr/bin/gdb"
		},
		{
			"name": "Debug Tests",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/cpp/build/board_test",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/cpp/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build Tests",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}
